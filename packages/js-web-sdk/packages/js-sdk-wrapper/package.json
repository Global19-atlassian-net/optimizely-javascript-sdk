{
  "name": "@optimizely/js-sdk-wrapper",
  "version": "0.1.0",
  "description": "Wrapper for Optimizely JavscriptSDK",
  "author": "Jordan Garcia <jordan@optimizely.com>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "./node_modules/.bin/ts-mocha --exit test/**",
    "build-browser-umd": "rm -rf dist && webpack",
    "cover": "nyc mocha test/**",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "prepublishOnly": "npm run build-browser-umd && npm test && npm run test-xbrowser"
  },
  "dependencies": {
    "@optimizely/optimizely-sdk": "^2.2.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/sinon": "^5.0.6",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^10.0.2",
    "nyc": "^13.1.0",
    "sinon": "^2.3.1",
    "ts-mocha": "^2.0.0",
    "typescript": "^3.1.6"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "lib/",
    "LICENSE",
    "CHANGELOG",
    "README.md"
  ]
}
